@page
@{
    Layout = "_Layout";
}
&emsp;&emsp;&emsp;&emsp;&emsp;&emsp;
<div class="col-12 col-md-8 col-lg-6 col-xl-5">
    <div class="card shadow-2-strong" style="border-radius: 1rem;">
        <div class="card-body p-5">

            <h3 class="mb-4 text-center">User Profile</h3>

            <div class="form-outline mb-4" >
                <strong>First Name:</strong> <span id="firstName"></span>
            </div>
            <div class="form-outline mb-4">
                <strong>Last Name:</strong> <span id="lastName"></span>
            </div>
            <div class="form-outline mb-4"> 
                <strong>Email:</strong> <span id="email"></span>
            </div>
            <div class="form-outline mb-4">
                <strong>Phone Number:</strong> <span
                    id="phoneNumber"></span>
            </div>
            <div class="form-outline mb-4">
                <strong>Role:</strong> <span id="role"></span>
            </div>
            <div class="form-outline mb-4">
                <strong>Created At:</strong> <span id="createdAt"></span>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    <script src="~/js/User/profile.js"></script>
}
